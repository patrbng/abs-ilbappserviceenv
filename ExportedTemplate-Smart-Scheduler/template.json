{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualNetworks_smartscheduler_vnet_name": {
            "defaultValue": "smartscheduler-vnet",
            "type": "String"
        },
        "networkSecurityGroups_smartscheduler_ase_NSG_externalid": {
            "defaultValue": "/subscriptions/6569e944-8e78-408b-8b35-94146ba1dfc8/resourceGroups/Smart-Scheduler/providers/Microsoft.Network/networkSecurityGroups/smartscheduler-ase-NSG",
            "type": "String"
        },
        "routeTables_smartscheduler_ase_Route_Table_externalid": {
            "defaultValue": "/subscriptions/6569e944-8e78-408b-8b35-94146ba1dfc8/resourceGroups/Smart-Scheduler/providers/Microsoft.Network/routeTables/smartscheduler-ase-Route-Table",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Network/virtualNetworks",
            "apiVersion": "2019-09-01",
            "name": "[parameters('virtualNetworks_smartscheduler_vnet_name')]",
            "location": "centralus",
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "192.168.250.0/23"
                    ]
                },
                "subnets": [
                    {
                        "name": "smartscheduler-subnet",
                        "properties": {
                            "addressPrefix": "192.168.250.0/24",
                            "networkSecurityGroup": {
                                "id": "[parameters('networkSecurityGroups_smartscheduler_ase_NSG_externalid')]"
                            },
                            "routeTable": {
                                "id": "[parameters('routeTables_smartscheduler_ase_Route_Table_externalid')]"
                            },
                            "delegations": [],
                            "privateEndpointNetworkPolicies": "Enabled",
                            "privateLinkServiceNetworkPolicies": "Enabled"
                        }
                    },
                    {
                        "name": "GatewaySubnet",
                        "properties": {
                            "addressPrefix": "192.168.251.0/24",
                            "delegations": [],
                            "privateEndpointNetworkPolicies": "Enabled",
                            "privateLinkServiceNetworkPolicies": "Enabled"
                        }
                    }
                ],
                "virtualNetworkPeerings": [],
                "enableDdosProtection": false,
                "enableVmProtection": false
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "apiVersion": "2019-09-01",
            "name": "[concat(parameters('virtualNetworks_smartscheduler_vnet_name'), '/GatewaySubnet')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/virtualNetworks', parameters('virtualNetworks_smartscheduler_vnet_name'))]"
            ],
            "properties": {
                "addressPrefix": "192.168.251.0/24",
                "delegations": [],
                "privateEndpointNetworkPolicies": "Enabled",
                "privateLinkServiceNetworkPolicies": "Enabled"
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "apiVersion": "2019-09-01",
            "name": "[concat(parameters('virtualNetworks_smartscheduler_vnet_name'), '/smartscheduler-subnet')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/virtualNetworks', parameters('virtualNetworks_smartscheduler_vnet_name'))]"
            ],
            "properties": {
                "addressPrefix": "192.168.250.0/24",
                "networkSecurityGroup": {
                    "id": "[parameters('networkSecurityGroups_smartscheduler_ase_NSG_externalid')]"
                },
                "routeTable": {
                    "id": "[parameters('routeTables_smartscheduler_ase_Route_Table_externalid')]"
                },
                "delegations": [],
                "privateEndpointNetworkPolicies": "Enabled",
                "privateLinkServiceNetworkPolicies": "Enabled"
            }
        }
    ]
}